<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="top.littlefogcat.clickerx.home">
    <!-- 网络 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 读存储 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!-- 写存储 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 保持唤醒 -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <!-- 解锁 -->
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />

    <application
        android:name="top.littlefogcat.clickerx.base.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="top.littlefogcat.clickerx.home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="top.littlefogcat.clickerx.base.ui.DummyActivity" />
    </application>

</manifest>